#spring propiertes
spring:
  aop:
    auto: true
    proxy-target-class: false
  messages:
    always-use-message-format: false
    basename: message
    cache-duration: -1
  mvc:
    dispatch-trace-request: false
    dispatch-options-request: true
  data:    
    web:
      pageable:
        default-page-size: 20
        max-page-size: 2000
        one-indexed-parameters: false
        page-parameter: page