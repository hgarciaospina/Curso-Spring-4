spring:
  datasource:
    url: jdbc:mysql://localhost:3306/mydb
    data-username: root
    data-password: root
    
  jpa:
    show-sql: true
    hibernate:
      ddl-auto: update
      naming:
        physical:
          strategy: org.hibernate.boot.model.naming.PhysicalNamingStrategyStandardImpl    
          
          
      hibernate:
      dialect: org.hibernate.dialect.MySQLDialect      