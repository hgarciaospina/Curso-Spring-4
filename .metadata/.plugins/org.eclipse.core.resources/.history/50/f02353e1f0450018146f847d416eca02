spring:
  datasource:
    url: jdbc:mysql://localhost:3306/mydb
    data-username: root
    data-password: root
    
  jpa:
    show-sql: true
    hibernate:
      ddl-auto: update
      naming:
          strategy: org.hibernate.cfg.ImprovedNamingStrategy
    propiertes:
      hibernate:
      dialect: org.hibernate.dialect.MySQLDialect      